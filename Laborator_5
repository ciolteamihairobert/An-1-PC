#include <stdio.h>
#include <stdlib.h>

int main()
{
//5.1
int n;
scanf("%d",&n);
printf("%d\n",n);
while(n>10)
{
    printf("%d\n",n-(n/10));;
    n=n-(n/10);
}
//5.2
int n1,s=0;
scanf("%d",&n1);
int i,j;
for (i=1;i<=n1;i++)
    s+=i;
printf("1+2+3+4+...+%d=%d",n1,s);
//5.3
int x;
scanf("%d",&x);
if(x>1){
for(i=0;i<x;i++)
    {for(j=0;j<x;j++)
       if(i==0||j==0||i==n-1||j=n-1)
       {
           printf("*");
       }
       printf("\n");}
}
else
{
    printf("numar prea mic");
}
//5.5
int n2;
scanf("%d",&n2);
for(i=1;i<=n2;i++){
    for(j=1;j<=n2;j++)
       if (i==j||j==1||j==n2)
          printf("*");
       else
          printf(" ");
        printf("\n");}
//5.7
for (i=0;i<=10;i++)
{for(j=0;j<=10;j++)
    printf("%d*%d=%d\n",i,j,i*j);
    printf("\n");}

    return 0;
}
